{
    "builds": [
      {
        "src": "myproject/wsgi.py",
        "use": "@vercel/python",
        "config": { 
          "maxLambdaSize": "15mb", 
          "runtime": "python3.12" 
        }
      }
      {
        "source": "/feedback/((?!general).*)",
        "destination": "/api/feedback/general"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "myproject/wsgi.py"
      }
    ]
  }
  